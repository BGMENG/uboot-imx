	            >;
        	};

	        pinctrl_lpuart0: lpuart0grp {
        	    fsl,pins = <
                	SC_P_UART0_RX_DMA_UART0_RX                  0x06000020
	                SC_P_UART0_TX_DMA_UART0_TX                  0x06000020
        	    >;
	        };

		pinctrl_typec: typecgrp {
        	    fsl,pins = <
	                SC_P_QSPI1A_SS0_B_LSIO_GPIO4_IO19	0x60
