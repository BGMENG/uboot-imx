                	SC_P_QSPI1A_DATA0_LSIO_GPIO4_IO26	0x00000021
	            >;
		};

	        pinctrl_usbotg2: usbotg2 {
        	    fsl,pins = <
                	SC_P_USB_SS3_TC1_CONN_USB_OTG2_PWR
	            >;
        	};

	        pinctrl_lpuart1: lpuart1grp {
